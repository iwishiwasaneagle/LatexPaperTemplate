@article{hashimoto_agentbased_2022,
  title = {An Agent-Based Model Reveals Lost Person Behavior Based on Data from Wilderness Search and Rescue},
  author = {Hashimoto, Amanda and Heintzman, Larkin and Koester, Robert and Abaid, Nicole},
  year = {2022},
  month = dec,
  journal = {Scientific Reports},
  volume = {12},
  number = {1},
  pages = {5873},
  issn = {2045-2322},
  doi = {10.1038/s41598-022-09502-4},
  urldate = {2022-06-28},
  abstract = {Abstract             Thousands of people are reported lost in the wilderness in the United States every year and locating these missing individuals as rapidly as possible depends on coordinated search and rescue (SAR) operations. As time passes, the search area grows, survival rate decreases, and searchers are faced with an increasingly daunting task of searching large areas in a short amount of time. To optimize the search process, mathematical models of lost person behavior with respect to landscape can be used in conjunction with current SAR practices. In this paper, we introduce an agent-based model of lost person behavior which allows agents to move on known landscapes with behavior defined as independent realizations of a random variable. The behavior random variable selects from a distribution of six known lost person reorientation strategies to simulate the agent's trajectory. We systematically simulate a range of possible behavior distributions and find a best-fit behavioral profile for a hiker with the International Search and Rescue Incident Database. We validate these results with a leave-one-out analysis. This work represents the first time-discrete model of lost person dynamics validated with data from real SAR incidents and has the potential to improve current methods for wilderness SAR.},
  langid = {english}
}

@article{haverkort_computing_2009,
  title = {Computing Visibility on Terrains in External Memory},
  author = {Haverkort, Herman and Toma, Laura and Zhuang, Yi},
  year = {2009},
  month = feb,
  journal = {ACM Journal of Experimental Algorithmics},
  volume = {13},
  issn = {1084-6654, 1084-6654},
  doi = {10.1145/1412228.1412233},
  urldate = {2022-06-08},
  abstract = {Given an arbitrary viewpoint               v               and a terrain, the visibility map or viewshed of               v               is the set of points in the terrain that are visible from               v               . In this article we consider the problem of computing the viewshed of a point on a very large grid terrain in external memory. We describe algorithms for this problem in the cache-aware and cache-oblivious models, together with an implementation and an experimental evaluation. Our algorithms are a novel application of the distribution sweeping technique and use               O               (               sort               (               n               )) I/Os, where               sort               (               n               ) is the complexity of sorting               n               items of data in the I/O-model. The experimental results demonstrate that our algorithm scales up and performs significantly better than the traditional internal-memory plane sweep algorithm and can compute visibility for terrains of 1.1 billion points in less than 4 hours on a low-cost machine compared to more than 32 hours with the internal-memory algorithm.},
  langid = {english}
}

@inproceedings{hayat_multiobjective_2017,
  title = {Multi-Objective {{UAV}} Path Planning for Search and Rescue},
