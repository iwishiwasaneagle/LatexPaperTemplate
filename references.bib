@inproceedings{waharte_supporting_2010,
  title = {Supporting {{Search}} and {{Rescue Operations}} with {{UAVs}}},
  booktitle = {2010 {{International Conference}} on {{Emerging Security Technologies}}},
  author = {Waharte, Sonia and Trigoni, Niki},
  year = {2010},
