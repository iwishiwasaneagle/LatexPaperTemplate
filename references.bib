  booktitle = {Proceedings of the {{Genetic}} and {{Evolutionary Computation Conference}} 2016},
  author = {{Perez-Carabaza}, Sara and {Besada-Portas}, Eva and {Lopez-Orozco}, Jose A. and {de la Cruz}, Jesus M.},
