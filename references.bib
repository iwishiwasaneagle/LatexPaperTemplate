@article{seric_lost_2021,
  title = {Lost {{Person Search Area Prediction Based}} on {{Regression}} and {{Transfer Learning Models}}},
  author = {{\v S}eri{\'c}, Ljiljana and Pinju{\v s}i{\'c}, Tomas and Topi{\'c}, Karlo and Bla{\v z}evi{\'c}, Tomislav},
  year = {2021},
